        "      self.bonus_atk = 0\n",
        "      self.total_atk = self.attack + self.bonus_atk\n",
        "      self.defense = card.defense\n",
        "      self.bonus_def = 0\n",
        "      self.total_atk = self.attack + self.bonus_atk\n",
        "      self.race = card.race\n",
        "      self.level = card.level\n",
        "      self.attribute = card.attribute\n",
        "      self.posChanged = True\n",
        "      self.equipped = []\n",
        "      self.can_atk = True\n",
        "      self.switched = False\n",
        "    \n",
